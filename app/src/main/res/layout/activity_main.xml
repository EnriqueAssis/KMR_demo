<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@drawable/gradient"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/kmr_map"
        android:layout_width="700sp"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="220dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/kmrmap" />

    <Button
        android:id="@+id/button_parse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"

        android:text="@string/get_battery_level"
        app:layout_constraintBottom_toBottomOf="@+id/kmr_map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.285"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/kmr_map" />

    <Button
        android:id="@+id/button_parse2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"

        android:text="@string/send_jason"
        app:layout_constraintBottom_toBottomOf="@+id/kmr_map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.282"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_parse" />

    <ImageView
        android:id="@+id/kuka"
        android:layout_width="103dp"
        android:layout_height="31dp"
        android:layout_marginTop="16sp"
        android:layout_marginEnd="16sp"
        android:contentDescription="@string/todo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kuka" />

    <TextView
        android:id="@+id/bottomBox3"
        android:layout_width="200sp"
        android:layout_height="200sp"


        android:layout_marginEnd="50dp"
        android:background="@drawable/box1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.982" />

    <TextView
        android:id="@+id/bottomBox2"
        android:layout_width="200sp"
        android:layout_height="200sp"


        android:layout_marginEnd="56dp"

        android:background="@drawable/box1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bottomBox3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.98" />

    <TextView
        android:id="@+id/bottomBox1"
        android:layout_width="200sp"
        android:layout_height="200sp"


        android:layout_marginEnd="44dp"

        android:background="@drawable/box1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bottomBox2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.981" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="104dp"
        android:text="Safety state"
        app:layout_constraintBottom_toBottomOf="@+id/bottomBox3"
        app:layout_constraintEnd_toEndOf="@+id/bottomBox3"
        app:layout_constraintTop_toTopOf="@+id/bottomBox3"
        app:layout_constraintVertical_bias="0.458" />

    <TextView
        android:id="@+id/text_battery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginEnd="56dp"
        android:text="@string/textview"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/battery"
        app:layout_constraintTop_toTopOf="@+id/bottomBox3"
        app:layout_constraintVertical_bias="0.122" />

    <ImageView
        android:id="@+id/battery"
        android:layout_width="53dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/text_battery"
        app:layout_constraintEnd_toEndOf="@+id/bottomBox3"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="@+id/bottomBox3"
        app:layout_constraintTop_toTopOf="@+id/text_battery"
        app:srcCompat="@drawable/battery"
        android:contentDescription="TODO" />

    "


    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="86dp"
        android:layout_height="63dp"
        android:layout_marginBottom="376dp"
        android:contentDescription="@string/todo"
        android:outlineProvider="none"
        app:layout_constraintBottom_toBottomOf="@+id/kmr_map"
        app:layout_constraintEnd_toEndOf="@+id/kmr_map"
        app:layout_constraintHorizontal_bias="0.705"
        app:layout_constraintStart_toStartOf="@+id/kmr_map"
        app:srcCompat="@drawable/kr3" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="86dp"
        android:layout_height="63dp"
        android:contentDescription="@string/todo"
        android:outlineProvider="none"
        app:layout_constraintBottom_toTopOf="@+id/imageButton6"
        app:layout_constraintEnd_toEndOf="@+id/kmr_map"
        app:layout_constraintHorizontal_bias="0.705"
        app:layout_constraintStart_toStartOf="@+id/kmr_map"
        app:layout_constraintTop_toTopOf="@+id/kmr_map"
        app:layout_constraintVertical_bias="0.827"
        app:srcCompat="@drawable/kr3" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="86dp"
        android:layout_height="63dp"
        android:contentDescription="@string/todo"
        android:outlineProvider="none"
        app:layout_constraintBottom_toTopOf="@+id/imageButton5"
        app:layout_constraintEnd_toEndOf="@+id/imageButton5"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/kmr_map"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:layout_constraintVertical_bias="0.489"
        app:srcCompat="@drawable/kr3" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="86dp"
        android:layout_height="63dp"
        android:contentDescription="@string/todo"
        android:outlineProvider="none"
        app:layout_constraintBottom_toBottomOf="@+id/kmr_map"
        app:layout_constraintEnd_toEndOf="@+id/imageButton4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/kmr_map"
        app:layout_constraintTop_toTopOf="@+id/imageButton2"
        app:layout_constraintVertical_bias="0.131"
        app:srcCompat="@drawable/kr3" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="63dp"
        android:layout_height="86dp"
        android:contentDescription="@string/todo"
        android:outlineProvider="none"
        app:layout_constraintBottom_toBottomOf="@+id/kmr_map"
        app:layout_constraintEnd_toEndOf="@+id/kmr_map"
        app:layout_constraintStart_toEndOf="@+id/imageButton1"
        app:layout_constraintTop_toTopOf="@+id/imageButton1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/kr3" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="63dp"
        android:layout_height="86dp"
        android:contentDescription="@string/todo"
        android:outlineProvider="none"
        app:layout_constraintBottom_toBottomOf="@+id/kmr_map"
        app:layout_constraintEnd_toEndOf="@+id/kmr_map"
        app:layout_constraintStart_toStartOf="@+id/kmr_map"
        app:layout_constraintTop_toTopOf="@+id/kmr_map"
        app:layout_constraintVertical_bias="0.057"
        app:srcCompat="@drawable/kr3" />"

    />


</androidx.constraintlayout.widget.ConstraintLayout>